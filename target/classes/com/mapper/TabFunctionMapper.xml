<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace定位接口-接口的完全限定名：包名.接口名-->
<mapper namespace="com.mapper.TabFunctionMapper">
    <select id="insertFunction" resultType="int" parameterType="java.util.Map">
        insert into tab_account(accountid,accountname)values(
            #{aid},#{aname}
        )
    </select>
</mapper>